{
    "extends": ["eslint:recommended"],
    "plugins": [
        "simple-import-sort"
    ],
    "parserOptions": {},
    "env": {"browser": true},
    "globals": {},
    "rules": {
        "indent": ["error", 2],
        "simple-import-sort/imports": "error", // import 並び替え
        "simple-import-sort/exports": "error" // export 並び替え
    }
}